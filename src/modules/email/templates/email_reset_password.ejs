<!-- BEGIN BODY // -->
<table border="0" cellpadding="0" cellspacing="0" width="600" id="templateBody">
  <tr>
    <td valign="top" class="bodyContainer">
      <table
        border="0"
        cellpadding="0"
        cellspacing="0"
        width="100%"
        class="mcnTextBlock"
        style="min-width: 100%"
      >
        <tbody class="mcnTextBlockOuter">
          <tr>
            <td valign="top" class="mcnTextBlockInner" style="padding-top: 9px">
              <table
                align="left"
                border="0"
                cellpadding="0"
                cellspacing="0"
                style="max-width: 100%; min-width: 100%"
                width="100%"
                class="mcnTextContentContainer"
              >
                <tbody>
                  <tr>
                    <td
                      valign="top"
                      class="mcnTextContent"
                      style="
                        padding-top: 0;
                        padding-right: 18px;
                        padding-bottom: 9px;
                        padding-left: 18px;
                      "
                    >
                      <h1>
                        <span style="font-size: 24px">Password Reset</span>
                      </h1>
                      <div
                        class="m_2860620098513552206FormattedEmail-divider"
                        style="
                          background: #9f9fa9;
                          height: 1px;
                          margin: 20px 0 28px;
                          width: 24px;
                        "
                      >
                        &nbsp;
                      </div>
                      <p><span style="font-size: 17px"><%= name %>,</span></p>
                      <p>
                        <span style="font-size: 17px">Token: <%= token %></span>
                      </p>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
        </tbody>
      </table>
    </td>
  </tr>
</table>
